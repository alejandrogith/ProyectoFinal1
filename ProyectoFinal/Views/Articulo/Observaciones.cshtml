@model IEnumerable<ProyectoFinal.Models.RecepcionDetalle>
@{
    ViewBag.Title = "Observaciones";
}

<h2>Observaciones</h2>


<div class="form-group">
    <div class="col-md-10">

        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.NombreEvaluador)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.Observacion)
                </th>



                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.NombreEvaluador)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Observacion)
                    </td>


                </tr>
            }

        </table>





       
    </div>
</div>
